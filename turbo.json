{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["es/**", "lib/**"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [".jest-cache/**"]
    }
  },
  "globalDependencies": ["tsconfig.json", "jest.config.ts", ".fatherrc.base.ts"]
}
